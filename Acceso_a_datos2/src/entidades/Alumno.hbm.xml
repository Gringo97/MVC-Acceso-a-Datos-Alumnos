<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="entidades.Alumno" table="alumnos">
		<id name="cod" type="int">
			<column name="cod" />
			<generator class="increment" />
		</id>

		<property name="dni" type="string">
			<column name="dni" length="64" not-null="true" />
		</property>
		
		<property name="nombre" type="string">
			<column name="nombre" length="64" not-null="true" />
		</property>
		
		<property name="apellido" type="string">
			<column name="apellido" length="64" not-null="true" />
		</property>
		
		<property name="telefono" type="int">
			<column name="telefono" length="9" not-null="true" />
		</property>
		
		<property name="nacionalidad" type="string">
			<column name="nacionalidad" length="64" not-null="true" />
		</property>
		
		<many-to-one name="titulacionAlumno" column="titulacion"
			not-null="true" />
			
	</class>

</hibernate-mapping>